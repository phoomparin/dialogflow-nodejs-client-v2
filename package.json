{
  "name": "dialogflow",
  "description": "Dialogflow API client for Node.js",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "author": "Google LLC",
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": "dialogflow/dialogflow-nodejs-client-v2",
  "main": "src/index.js",
  "files": [
    "protos",
    "src",
    "AUTHORS",
    "COPYING"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google dialogflow",
    "dialogflow",
    "Dialogflow API"
  ],
  "contributors": [
    "Alexander Fenster <fenster@google.com>",
    "Jorge del Casar <948953+jorgecasar@users.noreply.github.com>",
    "Luke Sneeringer <lukesneeringer@google.com>",
    "Matt Carroll <matthewcarroll@google.com>"
  ],
  "scripts": {
    "cover": "nyc --reporter=lcov mocha --require intelli-espower-loader test/*.js && nyc report",
    "docs": "repo-tools exec -- jsdoc -c .jsdoc.js",
    "generate-scaffolding": "repo-tools generate all && repo-tools generate lib_samples_readme -l samples/ --config ../.cloud-repo-tools.json",
    "lint": "repo-tools lint --cmd eslint -- src/ samples/ system-test/ test/",
    "prettier": "repo-tools exec -- prettier --write src/*.js src/*/*.js samples/*.js samples/*/*.js test/*.js test/*/*.js",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "test-no-cover": "repo-tools test run --cmd mocha -- test/*.js --no-timeouts",
    "test": "repo-tools test run --cmd npm -- run cover"
  },
  "dependencies": {
    "google-gax": "^0.16.0",
    "lodash.merge": "^4.6.0",
    "lodash.union": "^4.6.0",
    "protobufjs": "^6.8.0"
  },
  "devDependencies": {
    "@google-cloud/common": "^0.17.0",
    "@google-cloud/nodejs-repo-tools": "^2.1.1",
    "async": "^2.6.0",
    "codecov": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "extend": "^3.0.1",
    "ink-docstrap": "^1.3.0",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.0",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "through2": "^2.0.3"
  }
}
